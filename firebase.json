{
	"hosting": [
		{
			"public": "dist",
			"target": "affiliate",
			"headers": [
				{
					"source": "**/*",
					"headers": [
						{
							"key": "Access-Control-Allow-Origin",
							"value": "*"
						}
					]
				}
			],
			"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		},
		{
			"public": "dist",
			"target": "affiliate-dev",
			"headers": [
				{
					"source": "**/*",
					"headers": [
						{
